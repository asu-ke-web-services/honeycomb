{
    "name": "gios-asu/honeycomb",
    "description": "A framework for WordPress plugins",
    "license": "MIT",
    "version": "2.0.0",
    "authors": [
        {
            "name": "The Global Institute of Sustainability",
            "email": "giosweb@asu.edu"
        }
    ],
    "autoload": {
        "psr-4": {
            "Honeycomb\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Honeycomb\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "url": "https://github.com/gios-asu/nectary.git",
            "type": "git"
        }
    ],
    "require": {
        "php": ">=7.0",
        "gios-asu/nectary": "^1.0",
        "roave/security-advisories": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "satooshi/php-coveralls": "^2.0",
        "squizlabs/php_codesniffer": "^2.0",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.4.3",
        "wp-coding-standards/wpcs": "~0.14.0",
        "wimg/php-compatibility": "*"
    }
}
